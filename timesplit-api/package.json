{
  "name": "timesplit-api",
  "version": "0.0.1",
  "description": "Time Split API for RESTful API",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "NODE_ENV=production pm2 start node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Cory Billeaud <info@corybilleaud.com",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.0",
    "cors": "^2.8.1",
    "del": "^2.2.2",
    "ejs": "1.0.0",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "formidable": "^1.1.1",
    "fs": "0.0.1-security",
    "gridfs": "^1.0.0",
    "jsonwebtoken": "^7.3.0",
    "lokijs": "^1.4.3",
    "mongoose": "^4.8.5",
    "multer": "^1.3.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "path": "^0.12.7",
    "pug": "^2.0.0-beta11"
  },
  "devDependencies": {
    "@types/del": "^2.2.32",
    "@types/express": "^4.0.35",
    "@types/lokijs": "^1.2.30",
    "@types/multer": "0.0.33",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.16.1",
    "nodemon": "^1.11.0",
    "typescript": "^2.2.1"
  },
  "keywords": [
    "express",
    "es6",
    "rest",
    "api"
  ]
}
